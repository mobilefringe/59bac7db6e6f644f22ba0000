    <div id="mapsWrapper"></div>
    <script src="https://d3js.org/d3.v4.min.js"></script>
     <script src="http://www.sumbera.com/gist/js/leaflet/svg/scaled/leaflet.js"></script>
     <link href="https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.3/leaflet.css" rel="stylesheet">
    <script>
    // initialize the map
    var map;
    
    fnLoadMaps();
    
    function fnLoadMaps(){
        
        map = L.map('mapsWrapper').setView([15.433809, 100.755076], 5);
        
        L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token={accessToken}', {
            attribution: 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, <a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, Imagery © <a href="http://mapbox.com">Mapbox</a>',
            maxZoom: 20,
            id: 'jarfang.6d337a5b',
            accessToken: 'pk.eyJ1IjoiamFyZmFuZyIsImEiOiJhOWM5NzY4MTQ5MWU4MmVhZmMzNDlmZDk2ODk5YTNjOSJ9.3hGolcSbt9X8BJdlgqajDg'
        }).addTo(map);
        
        // Add an SVG element to Leaflet’s overlay pane
        var svg = d3.select(map.getPanes().overlayPane).append("svg"),
            g = svg.append("g").attr("class", "leaflet-zoom-hide");
        
        //Import the plane
        d3.xml("//mallmaverick.cdn.speedyrails.net/system/properties/svgmaps/000/000/028/original/Londonderry_DB_4.svg?1497642416", "image/svg+xml", function(svgImg) {
            
          map.getPanes().overlayPane.appendChild(svgImg.documentElement);
           
        });
}
        show_content();
    </script>