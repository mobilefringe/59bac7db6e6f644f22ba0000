<script>
    $(document).ready(function(e){
        init(e);
        
        loadMallDataCached(renderPage);
    });
    
    function renderPage(){
        var stores = getStoresList();
        console.log(stores);
        
        $('#mapplic').mapplic({
        	source: '//codecloud.cdn.speedyrails.net/sites/59bac7db6e6f644f22ba0000/application/json/1510937136000/mall copy.json',
        	height: 600,
        	mapfill:true,
        	minimap: false,
        	sidebar: true,
        	hovertip: true,
        	developer: true,
        	maxscale: 3
        });
        
        show_content();
    }
</script>
