                var temp_val = {};
                temp_val.id = val.id;
                temp_val.title= val.name;
                temp_val.color= "#b7a6bd";
                temp_val.show="true";
                mall_json.categories.push(temp_val);
            });
            mall_json.levels=[]; 
            // need to add the following for each floor we want to configure.
