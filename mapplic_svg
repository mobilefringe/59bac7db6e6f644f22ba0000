            // 	console.log("new location",location.x + ", " + location.y);
                
            	self.updateLocation(location.id);
            	self.moveTo(location.x, location.y, 1, 1000);
            // 	 console.log(map);
            });
            self = map.data('mapplic');
           
        }
        
